UnlitGeneric
{
	$basetexture "models/paradise/villa/toon_water_caustic_alpha"

	$alpha 0.3
	$additive 1

	$translate "[0 0]"

	Proxies
	{
		Sine
		{
			sineperiod 4
			sinemin 0
			sinemax 0.05
			timeoffset 0
			resultVar "$translate[0]"
		}

		Sine
		{
			sineperiod 5.5
			sinemin 0
			sinemax 0.05
			timeoffset 0
			resultVar "$translate[1]"
		}

		TextureTransform
		{
			translateVar "$translate"
			resultVar "$basetexturetransform"
		}
	}
}